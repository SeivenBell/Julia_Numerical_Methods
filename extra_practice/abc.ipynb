    "using WGLMakie\n",
    "\n",
    "fig = Figure()\n",
    "ax = Axis(fig[1, 1],\n",
    "    title=\"Tut_1 plotting\",\n",
    "    ylabel=\"Voltage\",\n",
    "    xlabel=\"Time\"\n",
    ")\n",
    "\n",
    "x = range(-4 * pi, 4 * pi, length=100)\n",
    "y = sin.(x) ./ x\n",
    "lines!(ax, x, y)\n",
    "fig"
   ]
